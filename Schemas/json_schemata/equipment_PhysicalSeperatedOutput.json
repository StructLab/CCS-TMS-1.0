{
  "$id": "https://ERJU/equipment_PhysicalSeperatedOutput",
  "$schema": "http://json-schema.org/draft-08/schema#",
  "definitions": {

  },
  "type": "object",
  "description": "PhysicalSeperatedOutput",
  "properties": {
    "id": {
      "type": "string",
      "description": "globally unique key (e.g. uuid)"
    },
    "statusTechnical": {
      "type": "string",
      "description": "General technical status of the equipment.",
      "enum": ["Unknown", "OK", "Warning", "FailureNonCritical", "FailureCritical"]
    },
    "statusTechnicalManufacturerSpecificValue": {
      "type": "array",
      "items": {
        "type": "string",
        "description": "Array of manufacturer-specific status codes."
       }
    },
    "label": {
      "type": "string"
    },
    "outputValue": {
      "type": "string",
      "enum": ["Unknown", "On", "Off"]
    }
  },
  "required": ["id", "label"],
  "additionalProperties": false
}
